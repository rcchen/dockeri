<h3>Dockeri <small>Settings</small></h3>
<div class="row">
  <div class="col-md-6">
  <%= form_for @settings, url: {action: "settings"}, html: {class: "form"} do |f| %>
      <div class="form-group">
          <label for="docker_protocol" class="control-label">Docker protocol</label>
          <%= f.select :docker_protocol, options_for_select([['TCP','tcp'],['Unix','unix']], @settings.docker_protocol), {}, { class: "form-control" } %>
      </div>
      <div class="form-group">
          <label for="docker_url" class="control-label">Docker URL</label>
          <%= f.text_field :docker_url, class: "form-control" %>
      </div>
      <div class="form-group">
          <label for="docker_port" class="control-label">Docker port</label>
          <%= f.text_field :docker_port, class: "form-control" %>
      </div>
      <%= f.submit "Save", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>
